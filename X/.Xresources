! -=################################## Xresource file ##################################=-
! {{{ Description
! Load with xrdb .Xresources
!
! Add to .xinitrc if you use startx
!
! Scrolling:
! Scroll Wheel
! Shift-PgUp/PgDn
!
! Copy/Paste:
! Mouse-Select             :  Copy to PRIMARY
! Shift + Mouse-Select     :  Copy to CLIPBOARD
!
! Middle-Click             :  Paste from PRIMARY
! Shift + Middle-Click     :  Paste from CLIPBOARD
!
! Ctrl+Shift+V             :  Paste from CLIPBOARD
! Ctrl+Alt+Shift+V         :  Paste from PRIMARY
!
!===============================================================================
! }}}
! General Options {{{1 =========================================================
! Make Tk applications slightly less ugly
*TkTheme                                      : clam
! Operate terminals in utf8 mode whether or not uxterm was called
*VT100.utf8                                   : 1
! Plenty of room for things like Vim :vsplit
*VT100*geometry                               : 160x50
*customization                                : -color
! -END- General Options 1}}}
! Color section {{{1 ===========================================================
!
!===============================================================================
!#include "/home/antenore/.Xcolors/raven_m9-dark"
!#include "/home/antenore/.Xcolors/gruvy"
!#include "/home/antenore/.Xcolors/LuciusBlack"
#include "/home/antenore/.Xcolors/Luciano"
!Debian (lovely)
#define DebianRed #D70A53
!   xterm specific
!*VT100.colorBDMode                            : True
!*VT100.colorULMode                            : True
! -END- Color Section 1}}}
! XFT Section {{{1
Xft.antialias                                 : 1
Xft.autohint                                  : 0
Xft.dpi                                       : 96
Xft.hinting                                   : 1
Xft.hintstyle                                 : hintfull
Xft.lcdfilter                                 : lcddefault
Xft.rgba                                      : rgb
*font xft:Inconsolata:size=9:antialias=true:hinting=true
*.font xft:Inconsolata:size=9:antialias=true:hinting=true
*font* xft:Inconsolata:size=9:antialias=true:hinting=true
! -END- XFT Section 1}}}
! URxvt section {{{1 ===========================================================
URxvt.termName                                : rxvt-unicode-256color
URxvt.imLocale                                : ch_FR.UTF-8
URxvt.loginShell                              : true
URxvt.saveLines                               : 90000
URxvt.geometry                                : 160x50
!Colors defined in the theme file raven-m9-dark
! For transparency enable the following including the foregrounnd background
!URxvt.transparent                             : true
URxvt.fading                                  : 05
!URxvt.shading                                 : 10
!URxvt.background                              : #002b36
!URxvt.foreground                              : #657b83
!URxvt.fadeColor                               : #002b36
!URxvt.depth                                   : 32
!URxvt.intensityStyles                         : false
URxvt.skipBuiltinGlyphs                       : True
URxvt.xftAntialias                            : true
URxvt.font                                    : xft:Inconsolata:size=9:antialias=true:hinting=true
URxvt.boldFont                                : xft:Inconsolata:size=9:antialias=true:hinting=true
URxvt.italicFont                              : xft:Inconsolata:size=9:antialias=true:hinting=true
! :Bind C-0, C-+ and C-= to activate normal, small, and big font size resp.
URxvt.keysym.C-0: command:\033]710;xft:Inconsolata:size=9:antialias=true:hinting=true\007
URxvt.keysym.C-minus: command:\033]710;xft:Inconsolata:size=7:antialias=true:hinting=true\007
!URxvt.keysym.C-equal: command:\033]710;-*-dina-medium-r-normal--16-*-*-*-*-*-*-*\007
URxvt.iconFile                                : /usr/share/icons/elementary/apps/32/terminal.svg
URxvt.perl-lib                                : /home/antenore/.urxvt
!URxvt.perl-ext-common                         : default,matcher,tabbed,clipboard,keyboard-select
URxvt.perl-ext-common                         : default,matcher,clipboard,keyboard-select,url-select
URxvt.keysym.M-Escape                         : perl:keyboard-select:activate
URxvt.keysym.M-s                              : perl:keyboard-select:search
URxvt.keysym.C-Delete                         : perl:matcher:last
URxvt.keysym.M-Delete                         : perl:matcher:list
URxvt.url-launcher                            : xdg-open
URxvt.matcher.button                          : 1
URxvt.matcher.rend.0                          : Uline Bold fg8
URxvt.keysym.M-u                              : perl:url-select:select_next
URxvt.url-select.autocopy                     : true
URvxt.url-select.button                       : 2
URxvt.url-select.launcher                     : /usr/bin/firefox %s
URxvt.url-select.underline                    : true
URxvt.print-pipe                              : cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
URxvt.pastableTabs                            : true
URxvt.urgentOnBell                            : true
URxvt.copyCommand                             : xclip -i -selection clipboard
URxvt.pasteCommand                            : xclip -o -selection clipboard
URxvt.keysym.S-C-C                            : perl:clipboard:copy
URxvt.keysym.S-C-V                            : perl:clipboard:paste
URxvt.keysym.S-M-v                            : perl:clipboard:paste_escaped
! Unbreak zsh keys in URxv
URxvt.keysym.Home                             : \033[1~
URxvt.keysym.End                              : \033[4~
URxvt.keysym.C-BackSpace                      : \033\010
URxvt.keysym.C-Tab                            : \033r
URxvt.selection.pattern-0                     : ([[:word:]]+)
URxvt.selection.pattern-1                     : ^(/[^:]+):\
URxvt.selection.pattern-2                     : \\|([^|]+)\\|
URxvt.cutchars                                : "`\"\'&()*,;<=>?@[]^{|}"
URxvt.scrollstyle                             : plain
URxvt.scrollBar                               : false
URxvt.jumpScroll                              : true
URxvt.secondaryWheel                          : true
URxvt.secondaryScreen                         : true
URxvt.scrollTtyOutput                         : false
URxvt.scrollWithBuffer                        : true
URxvt.scrollTtyKeypress                       : true
URxvt.tabbed.tabbar-fg                        : 7
URxvt.tabbed.tabbar-bg                        : 8
URxvt.tabbed.tab-fg                           : 0
URxvt.tabbed.tab-bg                           : 15
URxvt.cursorBlink                             : true
URxvt.cursorUnderline                         : True
! -END- URxvt Section 1}}}
